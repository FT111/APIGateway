{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Auth":
  {
    "Lifetime": "14400",
    "Issuer": "http://localhost:5235",
    "Audience": "http://localhost:5235",
    "Secret": "secret"
  },
  "CoreServices": {
    "InstanceStore": {
      "Identifier": "Core/EfStoreFactory",
      "SubIdentifier": "InstanceStore",
      "Configuration": {
        "ConnectionStrings": {
          "default": "secret"
        }
      }
    },
    "MessageAdapter": {
      "Identifier": "Core/RabbitSupervisorAdapter",
      "Configuration": {
        "Hostname": "clusterhost",
        "Username": "dev",
        "Password": "secret"
      }
    },
    "SupervisorStore": {
      "Identifier": "Core/EfStoreFactory",
      "SubIdentifier": "SupervisorStore",
      "Configuration": {
        "ConnectionStrings": {
          "default": "secret"
        }
      }
    },
    "PluginPackageManager": {
      "Identifier": "Core/LocalPackageManager",
      "Configuration": {
        "UnpackagedPath": "services/plugins",
        "PackagedPath": "routes/Static",
        "PackagesURL": "http://localhost:5235/",
        "PackagesPath": "static/packages",
        "RegisterLocalStaticRoute": true
      }
    },
  },
  "RabbitMQ": {
    "Hostname": "clusterhost",
    "Username": "dev",
    "Password":"secret"
  },
  "AllowedHosts": "*"
}
